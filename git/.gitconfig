[filter "lfs"]
	clean = git-lfs clean %f
	smudge = git-lfs smudge %f
	required = true
[push]
	default = simple
[url "git@github.com:"]
	insteadOf = https://github.com/
[fetch]
	prune = true
[remote "origin"]
	prune = true
[pull]
	rebase = false
[color]
    ui = auto
[color "status"]
    branch = white
[init]
	defaultBranch = main
[alias]
	cm = commit -am
	csm = commit -asS -m
